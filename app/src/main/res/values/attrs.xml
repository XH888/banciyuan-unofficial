        <attr name="x_tabSelectedTextColor" format="color"/>
        <attr name="x_tabPaddingStart" format="dimension"/>
        <attr name="x_tabPaddingTop" format="dimension"/>
        <attr name="x_tabPaddingEnd" format="dimension"/>
        <attr name="x_tabPaddingBottom" format="dimension"/>
        <attr name="x_tabPadding" format="dimension"/>
        <attr name="x_tabSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="XTabItem">
        <attr name="android:text"/>
        <attr name="android:icon"/>
        <attr name="android:layout"/>
    </declare-styleable>

    <declare-styleable name="FocusButton">
        <attr name="selectedColor" format="color" />
        <attr name="unselectedColor" format="color" />
        <attr name="selectText" format="string" />
        <attr name="unselectText" format="string" />
        <attr name="boardColor" format="color" />
        <attr name="selecedBoardColor" format="color" />
        <attr name="useDrawable" format="boolean" />
        <attr name="unselectedUseLine" format="boolean" />
        <attr name="selectedUseLine" format="boolean" />
        <attr name="isCheck" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ScaleImageView">
        <attr name="scale" format="float"/>
    </declare-styleable>


    <declare-styleable name="ScrollHeaderLayout">
        <attr name="fwidth" format="dimension" />
        <attr name="fheight" format="dimension" />
        <attr name="stable_height" format="dimension" />
    </declare-styleable>


